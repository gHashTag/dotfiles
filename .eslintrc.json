{
  parser: "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "strict": 0,
    'react/jsx-uses-react': 0,
    'react/jsx-uses-vars': 0,
    "indent": ["error", 2],
    "linebreak-style": [ "error", "unix" ],
    "quotes": [ "error", "single" ],
    "no-unused-vars": "error",
    "semi": [ "error", "never" ]
  }
}
